{
  "expo": {
    "name": "risedemo",
    "slug": "risedemo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app-assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app-assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0898A0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.risedemo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app-assets/adaptive-icon.png",
        "backgroundColor": "#0898A0"
      },
      "package": "com.anonymous.risedemo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
